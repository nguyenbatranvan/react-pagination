{
  "name": "react-headless-paginate",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "description": "Pagination for React",
  "author": "Van nguyen ba tran",
  "private": false,
  "sideEffects": false,
  "bugs": "https://github.com/nguyenbatranvan/react-pagination/issues",
  "repository": {
    "type": "git",
